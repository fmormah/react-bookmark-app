{
  "name": "bookmark-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.19.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.2.4",
    "eslint": "^8.32.0",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "stylelint": "^14.16.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "node-sass -r src/index.scss -o src && react-scripts start",
    "dev": "react-scripts start & node-sass src/index.scss -wo src",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!my-library-dir)/'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "sass": "^1.57.1",
    "stylelint-config-standard": "^29.0.0"
  }
}
